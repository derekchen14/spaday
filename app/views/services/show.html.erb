<h1>Show Services</h1>
<p>
  <strong>Service Name:</strong>
  <%= @service.name %>
</p>

<p>
  <strong>Instructors:</strong>
  <select>
    <% @service.instructors.each do |instructor| %>
      <option><%= instructor.name %></option>
    <% end%>
  </select>
</p>

<p>
  <%= link_to "Edit", edit_service_path(@service.id) %> |
  <%= link_to "Destroy", @service, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", services_path %>
</p>
